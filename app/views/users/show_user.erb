<h1>Welcome <%=@user.username%></h1>

<% if flash.has?(:message) %>
  <h3> <%= flash[:message] %> </h3>
  <br>
<% end %>

<h4>Here are all your beer experiences:</h4>
<!-- Style.all.order(:name).each -->
<% @user.experiences.order(created_at: :desc).each do |experience| %>
  <div>
    <p><%=experience.beer.name%> <br>
    Rating: <%=experience.rating%>  <button><a href="/experiences/<%=experience.id%>">View details</a></button></p>
  </div>
<% end %>
